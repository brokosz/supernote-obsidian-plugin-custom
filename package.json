{
  "name": "supernote-obsidian-plugin-custom",
  "version": "1.0.0",
  "description": "A customized version of the Supernote Obsidian plugin with support for custom output paths and note categorization",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "supernote",
    "note-taking"
  ],
  "author": "brokosz",
  "license": "MIT",
  "dependencies": {
    "jspdf": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.14.47",
    "obsidian": "^1.1.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}